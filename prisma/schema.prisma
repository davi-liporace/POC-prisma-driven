generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model lancamentos {
  id      Int      @id @default(autoincrement())
  valor   Int
  usuario String
  nome    String
  time    DateTime @default(now()) @db.Timestamp(6)
}

model usuarios {
  id      Int    @id @default(autoincrement())
  usuario String
}
